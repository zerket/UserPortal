                        "production": {
                            "browserTarget": "core:build:production",
                            "extraWebpackConfig": "apps/core/webpack.prod.config.js"
                        },
                        "en": {
                            "browserTarget": "core:build:en"
                        },
                        "ru": {
                            "browserTarget": "core:build:ru"
                        }
                    }
                },
                "serve-with-services": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            "nx serve core",
                            "nx serve service-overview",
                            "nx serve account-management"
                        ],
                        "parallel": true
                    }
                },
                "deploy": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            "nx build core",
                            "docker build --build-arg app_name=core --build-arg config=nginx.core.conf -t core .",
                            "docker rm --force core",
                            "docker run --name core -d -p 80:80 core"
