{
    "version": 1,
    "cli": {
        "defaultCollection": "@nrwl/angular"
    },
    "defaultProject": "wms-core",
    "schematics": {
        "@nrwl/angular:application": {
            "style": "less",
            "linter": "eslint",
            "unitTestRunner": "karma",
            "e2eTestRunner": "cypress",
            "strict": true
        },
        "@nrwl/angular:library": {
            "style": "less",
            "linter": "eslint",
            "unitTestRunner": "karma"
        },
        "@nrwl/angular:component": {
            "style": "less"
        },
        "@nrwl/react": {
            "application": {
                "babel": true
            }
        }
    },
    "projects": {
        "account-management": {
            "projectType": "application",
            "root": "apps/account-management",
            "sourceRoot": "apps/account-management/src",
            "prefix": "wms-core",
            "i18n": {
                "sourceLocale": "nl-NL",
                "locales": {
                    "nl": "apps/account-management/src/i18n/messages.nl.xlf",
                    "en": "apps/account-management/src/i18n/messages.en.xlf",
                    "ru": "apps/account-management/src/i18n/messages.ru.xlf"
                }
            },
            "architect": {
                "build": {
                    "builder": "ngx-build-plus:browser",
                    "options": {
                        "outputPath": "dist/apps/account-management",
                        "index": "apps/account-management/src/index.html",
                        "main": "apps/account-management/src/main.ts",
                        "polyfills": "apps/account-management/src/polyfills.ts",
                        "tsConfig": "apps/account-management/tsconfig.app.json",
                        "aot": true,
                        "assets": [
                            "apps/account-management/src/favicon.ico",
                            "apps/account-management/src/assets"
                        ],
                        "styles": [
                            "apps/account-management/src/styles.less"
                        ],
                        "scripts": [],
                        "extraWebpackConfig": "apps/account-management/webpack.config.js",
                        "i18nMissingTranslation": "warning",
                        "localize": [
                            "nl",
                            "en",
                            "ru"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "fileReplacements": [
                                {
                                    "replace": "apps/account-management/src/environments/environment.ts",
                                    "with": "apps/account-management/src/environments/environment.prod.ts"
                                }
                            ],
                            "optimization": true,
                            "outputHashing": "all",
                            "sourceMap": false,
                            "namedChunks": false,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "buildOptimizer": true,
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "2mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb",
                                    "maximumError": "10kb"
                                }
                            ],
                            "extraWebpackConfig": "apps/account-management/webpack.prod.config.js"
                        },
                        "nl": {
                            "localize": [
                                "nl"
                            ]
                        },
                        "en": {
                            "localize": [
                                "en"
                            ]
                        },
                        "ru": {
                            "localize": [
                                "ru"
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "ngx-build-plus:dev-server",
                    "options": {
                        "browserTarget": "account-management:build:nl",
                        "extraWebpackConfig": "apps/account-management/webpack.config.js",
                        "port": 3222
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "account-management:build:production",
                            "extraWebpackConfig": "apps/account-management/webpack.prod.config.js"
                        },
                        "en": {
                            "browserTarget": "account-management:build:en"
                        },
                        "ru": {
                            "browserTarget": "account-management:build:ru"
                        }
                    }
                },
                "deploy": {
                    "builder": "@nrwl/workspace:run-commands",
                    "options": {
                        "commands": [
                            "nx build account-management --prod",
                            "docker build --build-arg app_name=account-management --build-arg config=nginx.service.conf -t account-management .",
                            "docker rm --force account-management",
                            "docker run --name account-management -d -p 3222:80 account-management"
                        ],
                        "parallel": false
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "account-management:build"
                    }
                },
                "lint": {
                    "builder": "@nrwl/linter:eslint",
                    "options": {
                        "lintFilePatterns": [
                            "apps/account-management/src/**/*.ts",
                            "apps/account-management/src/**/*.html"
                        ]
                    }
                },
                "test": {
                    "builder": "ngx-build-plus:karma",
                    "options": {
                        "main": "apps/account-management/src/test.ts",
                        "tsConfig": "apps/account-management/tsconfig.spec.json",
                        "karmaConfig": "apps/account-management/karma.conf.js",
                        "polyfills": "apps/account-management/src/polyfills.ts",
                        "styles": [],
                        "scripts": [],
                        "assets": [],
                        "extraWebpackConfig": "apps/account-management/webpack.config.js"
                    }
                }
            }
        },
